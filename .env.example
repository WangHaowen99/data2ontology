# PostgreSQL 连接配置
PG_HOST=localhost
PG_PORT=5432
PG_DATABASE=your_database
PG_USER=your_user
PG_PASSWORD=your_password
PG_SCHEMA=public

# 分析配置
ANALYSIS_SIMILARITY_THRESHOLD=0.8
ANALYSIS_INCLUDE_VIEWS=false

# Neo4j 配置
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password
NEO4J_DATABASE=neo4j

# 输出配置
OUTPUT_DIR=./output
